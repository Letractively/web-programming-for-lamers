      <P style="MARGIN-TOP: 0px; MARGIN-BOTTOM: 0px"><FONT 
      face="Times New Roman" size=1><B>Expiration 
      year&nbsp;&nbsp;&nbsp;&nbsp;</B></FONT></P>
      <HR align=left width=92 color=#000000 noShade SIZE=1>
    </TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=middle><FONT face="Times New Roman" 
      size=1><B>Telecom<BR>Argentina</B></FONT><BR>
      <HR color=#000000 noShade SIZE=1>
    </TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=middle><FONT face="Times New Roman" 
      size=1><B>Publicom</B></FONT><BR>
      <HR color=#000000 noShade SIZE=1>
    </TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=middle><FONT face="Times New Roman" 
      size=1><B>Telecom<BR>Personal</B></FONT><BR>
      <HR color=#000000 noShade SIZE=1>
    </TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=middle><FONT face="Times New Roman" 
      size=1><B>Total<BR>consolidated</B></FONT><BR>
      <HR color=#000000 noShade SIZE=1>
    </TD></TR>
  <TR bgColor=#cceeff>
    <TD vAlign=top>
      <P style="MARGIN-LEFT: 1em; TEXT-INDENT: -1em"><FONT 
      face="Times New Roman" size=2>2007</FONT></P></TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=right><FONT face="Times New Roman" 
      size=2>1,416</FONT></TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=right><FONT face="Times New Roman" 
      size=2>—&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=right><FONT face="Times New Roman" 
      size=2>155</FONT></TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=right><FONT face="Times New Roman" 
      size=2>1,571</FONT></TD></TR>
  <TR>
    <TD vAlign=top>
      <P style="MARGIN-LEFT: 1em; TEXT-INDENT: -1em"><FONT 
      face="Times New Roman" size=2>2008</FONT></P></TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=right><FONT face="Times New Roman" 
      size=2>—&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=right><FONT face="Times New Roman" 
    size=2>2</FONT></TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=right><FONT face="Times New Roman" 
      size=2>—&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom align=right><FONT face="Times New Roman" 
    size=2>2</FONT></TD></TR>
  <TR>
    <TD vAlign=bottom><FONT size=1>&nbsp;</FONT></TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom>
      <HR color=#000000 noShade SIZE=1>
    </TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom>
      <HR color=#000000 noShade SIZE=1>
    </TD>
    <TD vAlign=bottom><FONT size=1>&nbsp;&nbsp;</FONT></TD>
    <TD vAlign=bottom>
      <HR color=#000000 noShade SIZE=1>
